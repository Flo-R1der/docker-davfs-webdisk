# Roots of the synchronization
root = /mnt/source
root = /mnt/webdrive

# Log actions
log = true

# Sync settings
fastcheck = true
auto = true
batch = true
times = true
prefer = newer

# Ignore 
ignore = Name temp.*
ignore = Name .*
ignore = Name *.tmp
ignore = Path lost+found

# Daemonize 
repeat = 30