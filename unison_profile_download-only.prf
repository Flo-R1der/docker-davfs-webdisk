# Roots of the synchronization
root = /mnt/local
root = /mnt/webdrive

# Log actions
log = true

# Sync settings
fastcheck = true
auto = true
batch = true
times = true
owner = true
group = true
#prefer = newer
prefer = /mnt/webdrive
force = /mnt/webdrive
nocreation = /mnt/webdrive
nodeletion = /mnt/webdrive

# Ignore 
ignore = Name temp.*
ignore = Name .*
ignore = Name *.tmp
ignore = Path lost+found

# Daemonize 
repeat = 30